<div class="modal-header">
    <h4 class="modal-title">{{title}}</h4>
</div>
<div class="modal-body">
    {{{body}}}
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">{{reject-text}}</button>
    <button type="button" class="btn btn-primary js-accept" data-dismiss="modal">{{accept-text}}</button>
</div>
